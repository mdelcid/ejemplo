package diagrama;

public class Principal {

	public static void main(String[] args)
	{
		Triangulo t = new Triangulo();;
		t.setBase(4);
		t.setAltura(5);
		System.out.print(t.getArea());

		Circulo c = new Circulo();
		c.setRadio(5);
		System.out.println(c.getArea());
		System.out.println(c.getPerimetro());
	}

}
